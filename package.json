  "scripts": {
    "start": "yarn run swagger && node scripts/start.js",
    "download-swagger-claim-api":
      "download https://artifactory.gitlab.bcs.ru/artifactory/bcs-main-snapshots/ru/bcs/creditmarkt/claim-service-root/1.0-SNAPSHOT/claim-service-root-1.0-SNAPSHOT-api.json > ./target/claim-service-root-1.0-SNAPSHOT-api.json",
    "swagger-claim-api":
      "yarn run download-swagger-claim-api && swagger-to-js-api --basePath /api/api/claim/v1 -t -f -n api-claim-service -o node_modules/api-claim-service -i ./target/claim-service-root-1.0-SNAPSHOT-api.json",
    "download-swagger-forms-api":
      "download https://artifactory.gitlab.bcs.ru/artifactory/bcs-main-snapshots/ru/bcs/creditmarkt/form-service/1.0-SNAPSHOT/form-service-1.0-SNAPSHOT-api.json > ./target/form-service-1.0-SNAPSHOT-api.json",
    "swagger-forms-api":
      "yarn run download-swagger-forms-api && swagger-to-js-api --basePath /api/api/form/v1 -t -f -n api-form-service -o node_modules/api-form-service -i ./target/form-service-1.0-SNAPSHOT-api.json",
    "download-swagger-product-catalog-api":
      "download https://artifactory.gitlab.bcs.ru/artifactory/bcs-main-snapshots/ru/bcs/creditmarkt/product-catalog-root/1.0-SNAPSHOT/product-catalog-root-1.0-SNAPSHOT-api.json > ./target/product-catalog-root-1.0-SNAPSHOT-api.json",
    "swagger-product-catalog-api":
      "yarn run download-swagger-product-catalog-api && swagger-to-js-api --basePath /api/api/catalog/v1 -t -f -n api-product-catalog-service -o node_modules/api-product-catalog-service -i ./target/product-catalog-root-1.0-SNAPSHOT-api.json",
    "swagger":
      "mkdirp ./target && yarn run swagger-forms-api && yarn run swagger-claim-api && yarn run swagger-product-catalog-api",
    "build": "yarn run swagger && node scripts/build.js",
    "build-storybook": "build-storybook -c .storybook",
    "test": "node scripts/test.js --env=jsdom",
    "test:debug":
      "./node/node --inspect-brk scripts/test.js --runInBand  --env=jsdom",
    "test:multiple":
      "yarn run test -- --capabilities.shardTestFiles=true --capabilities.maxInstances=3",
    "protractor": "./node/node node_modules/.bin/protractor protractor-conf.js",
    "protractor:debug":
      "./node/node --inspect-brk node_modules/.bin/protractor protractor-conf.js",
    "flow-typed": "flow-typed",
    "flow-coverage":
      "flow-coverage-report -i 'src/**/*.js' -t html -o reports/flow-coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint:styles": "stylelint \"src/**/styled.js\"",
    "lint:js":
      "prettier --list-different \"src/**/*.js\" \"config/**/*.js\" package.json .babelrc .eslintrc && eslint --max-warnings=0 --no-ignore  \"src/**/*.js\"",
    "prepare-for-commit":
      "prettier --write \"src/**/*.js\" \"src/**/*.css\" \"config/**/*.js\" \"protractor_e2e/**/*.js\" package.json .babelrc .eslintrc && eslint --fix \"src/**/*.js\" \"protractor_e2e/**/*.js\" && yarn lint:styles  ",
    "precommit": "yarn lint:styles && yarn lint:js"
  },
  "proxy": {
    "/api/": {
      "target": "http://gateway-service.192.168.99.100.xip.io/",
      "changeOrigin": true,
      "secure": false,
      "logLevel": "debug"
    }
  }
}
